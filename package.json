  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "gen": "apollo service:download --endpoint=https://api.graph.cool/simple/v1/cjre1ft0j65ik0122n839025k ./gen/schema.json",
